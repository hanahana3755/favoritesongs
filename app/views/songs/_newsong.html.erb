<h1>New Song</h1>
       <%= form_with model:song, local:true do |f| %>
          <div class="form-group">
            <label>song name
              <%= f.text_field:song_name, class:"form-control" %>
            </label>
          </div>
          <div class="form-group">
            <label>opinion
              <%= f.text_area:opinion, class:"form-control" %>
            </label>
          </div>
          <div class="form-group">
            <%= f.submit 'Create Song', class:"btn btn-success" %>
            <% end %>
          </div>